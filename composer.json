{
    "name": "mstroink/cakephp-google-analytics",
    "description": "A CakePHP package to retrieve Google Analytics data. Based on Spatie Laravel Analytics",
    "type": "cakephp-plugin",
    "keywords": [
        "google",
        "analytics",
        "retrieve",
        "reports",
        "cakephp"
    ],
    "homepage": "https://github.com/mstroink/cakephp-google-analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://murze.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "cakephp/cakephp": "~4.2.0",
        "cakephp/chronos": "^2.1",
        "google/apiclient": "^2.0|^2.7",
        "spatie/macroable": "^2.0",
        "symfony/cache": "^4.3|^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Analytics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Analytics\\Tests\\": "tests",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    
    "minimum-stability": "dev",
    "prefer-stable": true
}
